<table>
    <tr>
        <td>景区名称</td>
        <td>订单总数</td>
        <td>成人总数</td>
        <td>儿童总数</td>
        <td>总价</td>
    </tr>
<% @table.each do |r| %>
    <tr>
        <td><%= r.spot_name %></td>
        <td><%= r.count_sum %></td>
        <td><%= r.adult_ticket_sum %></td>
        <td><%= r.child_ticket_sum %></td>
        <td><%= r.price_sum %></td>
    </tr>
<% end %>
</table>

<p id = "pdf_link" > <%= link_to "PDF下载", reports_agent_spot_output_path(:format => "pdf",:type => params[:type],
:year => params[:year],:month => params[:month],:day => params[:day],:week => params[:week]),{"data-popup" => true}  %></p>

