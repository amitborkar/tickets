<fieldset class="inputs">
  <table style="border:none;">
    <tr style="border:none;">
      <td>报表类型<%= select_tag :type, options_for_select([["日", "day"], ["周", "week"], ["月", "month"]], params[:type]), :id =>"output_report_type" %></td>
      <td>
        <span>年 <%= select_tag :year, options_for_select([[2011, 2011], [2010, 2010]], params[:year]) %></span>
        <span>月 <%= select_tag :month, options_for_select([[2011, 2011]], params[:month]) %></span>
        <span>日 <%= select_tag :day, options_for_select([[2011, 2011]], params[:day]) %></span>
        <span>周 <%= select_tag :week, options_for_select([[2011, 2011]], params[:week]) %></span>
      </td>
      <td>
        <fieldset class="buttons">
          <%= submit_tag "查询"%>
        </fieldset>
      </td>
    </tr>
  </table>
</fieldset>
