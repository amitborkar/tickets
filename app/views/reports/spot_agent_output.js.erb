<table>
    <tr>
        <td>旅行社名称</td>
        <td>订单总数</td>
        <td>成人总数</td>
        <td>儿童总数</td>
        <td>总价</td>
    </tr>
<% @table.each do |r| %>
    <tr class="<%= cycle("trwrite","trgray") %>">
        <td><%= r.agent_name %></td>
        <td><%= r.count_sum %></td>
        <td><%= r.adult_ticket_sum.to_i %></td>
        <td><%= r.child_ticket_sum.to_i %></td>
        <td><%= r.price_sum.to_i %></td>
    </tr>
<% end %>
</table>

<table class="formtable01">
        <tr class="formtable01">
            <td class="formtable01" style="text-align:center; ">
            <%= button_to_new_window "PDF下载", reports_spot_agent_output_path(:format => "pdf",:type => params[:type],
               :year => params[:year],:month => params[:month],:day => params[:day],:week => params[:week]),{:class => "btn", :style=> "float:none;"}  %>
            </td>
        </tr>
</table>


