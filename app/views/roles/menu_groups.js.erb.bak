[
    <% @menu_groups.each do |menu_group| %>
    {attributes:{id:" <%= menu_group.id %>"},state:"open", data:"<%= menu_group.name %>",children:[
        <%menu_group.menus.each_with_index do |menu, y|
     if (y == menu_group.menus.length-1)%>
        {attributes:{id:" <%= menu.id %> "},state:"open",data:" <%= menu.name %> "  }
    ]} ,
    <%else%>
    {attributes:{id:" + <%= menu.id %> "},state:"open",data:" <%= menu.name %> "  },
    <% end %>
    <% end %>
    <% end %>
]